{
  "name": "cartouche",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && CI=test npm t"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.6.0",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
